    "import face_recognition\n",
    "import cv2\n",
    "import numpy as np\n",
    "\n",
    "# Get a reference to webcam #0 (the default one)\n",
    "video_capture = cv2.VideoCapture(0)\n",
    "\n",
    "# Load a sample picture and learn how to recognize it.\n",
    "#male_image = face_recognition.load_image_file(\"/Users/mac/Desktop/recognition2/imeags/female.jpg\")\n",
    "#male_face_encoding = face_recognition.face_encodings(female_image)[0]\n",
    "\n",
    "# Load a sample picture and learn how to recognize it.\n",
    "# here you have to specify the path and change the names accordingly. The error can be resolved by changing the path and names accordingly. 
    "hamza2_image = face_recognition.load_image_file(\"C:\\Users\\Blue\\Desktop\\project\\new/hamza2.jpg\")\n",
    "hamza2_face_encoding = face_recognition.face_encodings(hamza2_image)[0]\n",
    "\n",
    "# Load a sample picture and learn how to recognize it.\n",
    "sami_image = face_recognition.load_image_file(\"C:\\Users\\Blue\\Desktop\\project\\new/sami.jpeg\")\n",
    "sami_face_encoding = face_recognition.face_encodings(sami_image)[0]\n",
    "\n",
    "# Load a sample picture and learn how to recognize it.\n",
    "abdullah_image = face_recognition.load_image_file(\"C:\\Users\\Blue\\Desktop\\project\\new/abdullah.JPG\")\n",
    "abdullah_face_encoding = face_recognition.face_encodings(abdullah_image)[0]\n",
    "\n",
    "\n",
    "# Load a sample picture and learn how to recognize it.\n",
    "Shafiullah_image = face_recognition.load_image_file(\"C:\\Users\\Blue\\Desktop\\project\\new/Shafiullah.JPG\")\n",
    "Shafiullah_face_encoding = face_recognition.face_encodings(Shafiullah_image)[0]\n",
    "\n",
    "\n",
    "# Load a sample picture and learn how to recognize it.\n",
    "IslamHussain_image = face_recognition.load_image_file(\"C:\\Users\\Blue\\Desktop\\project\\new/IslamHussain.JPG\")\n",
    "IslamHussain_face_encoding = face_recognition.face_encodings(IslamHussain_image)[0]\n",
    "\n",
    "# Create arrays of known face encodings and their names\n",
    "known_face_encodings = [\n",
    "    #male_face_encoding,\n",
    "    hamza2_face_encoding,\n",
    "    sami_face_encoding,\n",
    "    abdullah_face_encoding,\n",
    "    Shafiullah_face_encoding,\n",
    "    IslamHussain_face_encoding,\n",
    "]\n",
    "known_face_names = [\n",
    " #   \"male\",\n",
    "    \"hamza2\",\n",
    "    \"sami\",\n",
    "   \"abdullah\",\n",
    "    \"Shafiullah\",\n",
    "    \"IslamHussain\"\n",
    "]\n",
    "\n",
    "# Initialize some variables\n",
    "face_locations = []\n",
    "face_encodings = []\n",
    "face_names = []\n",
    "process_this_frame = True\n",
    "\n",
    "while True:\n",
    "    # Grab a single frame of video\n",
    "    ret, frame = video_capture.read()\n",
    "\n",
    "    # Resize frame of video to 1/4 size for faster face recognition processing\n",
    "    small_frame = cv2.resize(frame, (0, 0), fx=0.25, fy=0.25)\n",
    "\n",
    "    # Convert the image from BGR color (which OpenCV uses) to RGB color (which face_recognition uses)\n",
    "    rgb_small_frame = small_frame[:, :, ::-1]\n",
    "\n",
    "    # Only process every other frame of video to save time\n",
    "    if process_this_frame:\n",
    "        # Find all the faces and face encodings in the current frame of video\n",
    "        face_locations = face_recognition.face_locations(rgb_small_frame)\n",
    "        face_encodings = face_recognition.face_encodings(rgb_small_frame, face_locations)\n",
    "\n",
    "        face_names = []\n",
    "        for face_encoding in face_encodings:\n",
    "            # See if the face is a match for the known face(s)\n",
    "            matches = face_recognition.compare_faces(known_face_encodings, face_encoding)\n",
    "            name = \"Unknown\"\n",
    "\n",
    "            # # If a match was found in known_face_encodings, just use the first one.\n",
    "            # if True in matches:\n",
    "            #     first_match_index = matches.index(True)\n",
    "            #     name = known_face_names[first_match_index]\n",
    "\n",
    "            # Or instead, use the known face with the smallest distance to the new face\n",
    "            face_distances = face_recognition.face_distance(known_face_encodings, face_encoding)\n",
    "            best_match_index = np.argmin(face_distances)\n",
    "            if matches[best_match_index]:\n",
    "                name = known_face_names[best_match_index]\n",
    "\n",
    "            face_names.append(name)\n",
    "\n",
    "    process_this_frame = not process_this_frame\n",
    "\n",
    "\n",
    "    # Display the results\n",
    "    for (top, right, bottom, left), name in zip(face_locations, face_names):\n",
    "        # Scale back up face locations since the frame we detected in was scaled to 1/4 size\n",
    "        top *= 4\n",
    "        right *= 4\n",
    "        bottom *= 4\n",
    "        left *= 4\n",
    "\n",
    "        # Draw a box around the face\n",
    "        cv2.rectangle(frame, (left, top), (right, bottom), (0, 0, 255), 2)\n",
    "\n",
    "        # Draw a label with a name below the face\n",
    "        cv2.rectangle(frame, (left, bottom - 35), (right, bottom), (0, 0, 255), cv2.FILLED)\n",
    "        font = cv2.FONT_HERSHEY_DUPLEX\n",
    "        cv2.putText(frame, name, (left + 6, bottom - 6), font, 1.0, (255, 255, 255), 1)\n",
    "\n",
    "    # Display the resulting image\n",
    "    cv2.imshow('Video', frame)\n",
    "\n",
    "    # Hit 'q' on the keyboard to quit!\n",
    "    if cv2.waitKey(1) & 0xFF == ord('q'):\n",
    "        break\n",
    "\n",
    "# Release handle to the webcam\n",
    "video_capture.release()\n",
    "cv2.destroyAllWindows()"
   ]
